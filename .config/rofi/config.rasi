@theme "/dev/null"

configuration {
	location : 0;
	anchor : north;
}


* {
	background-color : rgb(0,0,0);
	text-color: white;
}

window {
	background-color : rgb(0,0,0);
}

element {
	children: [ element-icon, element-text ];
}


element selected, element-icon selected, element-text selected {
	background-color : rgb(50,50,50);
}
